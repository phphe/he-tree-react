{
  "name": "he-tree-react",
  "version": "1.3.0",
  "author": "phphe <phphe@outlook.com> (https://github.com/phphe)",
  "description": "React draggable sortable tree component.",
  "keywords": [
    "draggable tree",
    "sortable tree",
    "React component"
  ],
  "repository": "https://github.com/phphe/he-tree-react.git",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "format": "biome format --write .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "helper-js": "^3.1.5",
    "react-base-virtual-list": "^1.2.9"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@biomejs/biome": "^1.9.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "jsdom": "^28.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}